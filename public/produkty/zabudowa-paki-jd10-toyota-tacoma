  let siteLightbox = null;

  function initSiteLightbox() {
    if (typeof GLightbox === "undefined") {
      return;
    }
    if (siteLightbox && typeof siteLightbox.destroy === "function") {
      siteLightbox.destroy();
    }
    siteLightbox = GLightbox({
      selector: '.glightbox',
      touchNavigation: true,
      loop: true,
      autoplayVideos: true
    });
  }

  window.addEventListener('load', function() {
  appMain(); animationMain(); initSiteLightbox();
  });
  const swup = new Swup({
    animateHistoryBrowsing: true, 
    plugins: [
      new SwupProgressPlugin(),
      new SwupBodyClassPlugin()
    ]
  });
  window.swup = swup;
  swup.hooks.on('page:view', () => {
       appMain(); animationMain(); initSiteLightbox();
  }, { before: true });
</script> 
